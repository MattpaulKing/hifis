{
  "name": "hifis",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "huh": "ts-node -h",
    "build": "vite build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "db": "sst shell drizzle-kit",
    "db:migrate": "tsx node_modules/drizzle-kit/bin.cjs migrate",
    "db:push": "tsx node_modules/drizzle-kit/bin.cjs push",
    "db:start": "docker compose up",
    "db:studio": "drizzle-kit studio",
    "dev": "vite dev",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "preview": "vite preview",
    "test": "npm run test:unit -- --run && npm run test:e2e",
    "test:e2e": "playwright test",
    "test:unit": "vitest"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.4",
    "@playwright/test": "^1.49.1",
    "@sveltejs/adapter-node": "^5.2.11",
    "@sveltejs/kit": "^2.15.1",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@types/node": "^22.10.2",
    "autoprefixer": "^10.4.20",
    "drizzle-kit": "^0.30.1",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.46.1",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "svelte": "^5.16.0",
    "svelte-check": "^4.1.1",
    "sveltekit-search-params": "^3.0.0",
    "sveltekit-superforms": "^2.22.1",
    "tailwindcss": "^3.4.17",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.19.0",
    "valibot": "1.0.0-beta.9",
    "vite": "^6.0.6",
    "vite-plugin-kit-routes": "^0.7.4",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.6.12",
    "@hono/node-server": "^1.13.7",
    "@openauthjs/openauth": "^0.2.6",
    "@skeletonlabs/tw-plugin": "^0.4.0",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.38.3",
    "drizzle-valibot": "^0.3.1",
    "pg": "^8.13.1",
    "postgres": "^3.4.5",
    "sst": "3.4.16",
    "svelte-grid-extended": "^1.2.1",
    "svelte-kit-sst": "2.43.5"
  }
}

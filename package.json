{
  "name": "hifis",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "huh": "ts-node -h",
    "build": "vite build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "db": "sst shell drizzle-kit",
    "db:migrate": "tsx node_modules/drizzle-kit/bin.cjs migrate",
    "db:push": "tsx node_modules/drizzle-kit/bin.cjs push",
    "db:start": "docker compose up",
    "db:studio": "drizzle-kit studio",
    "dev": "vite dev",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "preview": "vite preview",
    "test": "npm run test:unit -- --run && npm run test:e2e",
    "test:e2e": "playwright test",
    "test:unit": "vitest"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.1",
    "@playwright/test": "^1.54.2",
    "@sveltejs/adapter-node": "^5.2.13",
    "@sveltejs/kit": "^2.27.0",
    "@sveltejs/vite-plugin-svelte": "^5.1.1",
    "@types/node": "^22.17.0",
    "autoprefixer": "^10.4.21",
    "drizzle-kit": "^0.30.6",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-svelte": "^2.46.1",
    "globals": "^15.15.0",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "svelte": "^5.37.2",
    "svelte-check": "^4.3.0",
    "sveltekit-search-params": "^3.0.0",
    "sveltekit-superforms": "^2.27.1",
    "tailwindcss": "^3.4.17",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.38.0",
    "valibot": "1.0.0-beta.9",
    "vite": "^6.3.5",
    "vite-plugin-kit-routes": "^0.7.6",
    "vitest": "^2.1.9"
  },
  "dependencies": {
    "@lucide/svelte": "^0.482.0",
    "@neodrag/svelte": "3.0.0-next.7",
    "@openauthjs/openauth": "^0.2.7",
    "@skeletonlabs/tw-plugin": "^0.4.1",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "dotenv": "^16.6.1",
    "drizzle-orm": "^0.38.4",
    "drizzle-valibot": "^0.3.1",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "sst": "3.4.16",
    "svelte-grid-extended": "^1.2.1",
    "svelte-kit-sst": "2.43.5"
  }
}
